/**
 * @file
 * SMS Panel UI un/installation hooks.
 */

/**
 * Implementation of hook_enable().
 */
function sms_panel_ui_enable() {
  drupal_set_message(st('The <a href="!link">SMS Panel UI</a> has been successfully enabled.',
    array(
      '!link' => url('admin/smsframework/panel'),
    )
  ));
}

